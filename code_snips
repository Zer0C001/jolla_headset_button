import dbus
ses_bus=dbus.SessionBus()
proxy=ses_bus.get_object('org.mpris.MediaPlayer2.jolla_mediaplayer','/org/mpris/MediaPlayer2')
iface=dbus.Interface(proxy,'org.mpris.MediaPlayer2.Player')
iface.Stop()


from dbus.mainloop.glib import DBusGMainLoop
import dbus
import re
dbus_loop = DBusGMainLoop()
ses_bus=dbus.SessionBus(mainloop=dbus_loop)
def dbus_handler():
  pass

players=[]
for item in ses_bus.list_names():
    if re.match('org.mpris.MediaPlayer2',item):
      players.append(item)

print players


